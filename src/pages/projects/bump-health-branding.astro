            <span class="bg-gray-100 text-gray-800 text-sm font-medium px-3 py-1.5 rounded-full">Art Director</span>
          </div>
        </div>
      </section>

      <!-- Project Details -->
      <section class="py-16 px-6">
        <div class="max-w-6xl mx-auto">
