                <div
                  class="w-12 h-12 bg-gray-100 rounded-xl flex items-center justify-center mb-6"
                >
                  <svg
                    class="w-6 h-6 text-gray-600"
                    fill="none"
                    stroke="currentColor"
                    viewBox="0 0 24 24"
                  >
                    <ellipse cx="12" cy="6" rx="8" ry="3" stroke-width="2"
                    ></ellipse>
                    <ellipse cx="12" cy="12" rx="8" ry="3" stroke-width="2"
